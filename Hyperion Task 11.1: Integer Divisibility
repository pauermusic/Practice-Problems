int = int(input('Please enter an integer: '))

if int % 2 == 0 and int % 5 == 0:
    print('Your integer is divisible by both 2 and 5.')
elif int % 2 == 0 or int % 5 == 0:
    print('Your integer is divisible by one of either 2 or 5.')
elif int % 2 != 0 and int % 5 != 0:
    print('Your integer is not divisible by 2 nor 5.')
