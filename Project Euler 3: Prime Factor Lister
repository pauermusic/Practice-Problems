## Creates a list of prime factors of an entered number

## 1. Lists all factors of a number
testnum = int(input('Enter Number Here: '))

def factorlist(x):
    for i in (range(1,x)):
        if x % i == 0:
            listoffactors.append(i)

listoffactors = []
factorlist(testnum)

## 2. Out of all factors, prints out the prime ones 

def primetest(i):
    noms = []
    for j in range(1, i+1):
        if i % j == 0:
            noms.append(j)
    if len(noms) == 2:
        print(i)

for k in listoffactors:
    primetest(k)

close = input('Operation Complete')




