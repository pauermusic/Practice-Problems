## Creates a list of prime factors of an entered number

testnum = int(input('Enter Number Here: '))
listoffactors = []

def factorlist(x):
    for i in reversed(range(1,(x//2)+1)):
        if x % i == 0 and i != 1:
            listoffactors.append(i)

factorlist(testnum)
#print(listoffactors)

def primetest(i):
    noms = []
    for j in range(1, i+1):
        if i % j == 0:
            noms.append(j)
    if len(noms) == 2:
        print(i)

for k in listoffactors:
    primetest(k)

close = input('Operation Complete')
