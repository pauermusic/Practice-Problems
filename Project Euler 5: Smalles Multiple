## Finds the number which is a multiple of all the numbers from 1 to 20.


def divtest(num):
    a = 1
    while a <= 20:
        if num % a == 0:
            a += 1
            continue
        else:
            return False
    return True



for i in range(1, 10000000000000000):
    if divtest(i) == True:
        print(i)
        break
