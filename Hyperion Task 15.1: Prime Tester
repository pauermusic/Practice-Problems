x = int(input('Enter your integer: '))
if x <= 1:
    print(str(int) + ' is not a prime number. Prime numbers are always greater than 1.')

prime = ''
for i in range(2, x):
    if x % i == 0:
        print(str(x) + ' is not a prime number.')
        prime = 'false'
        break

if prime != 'false':
    print(str(x) + ' is a prime number.')
